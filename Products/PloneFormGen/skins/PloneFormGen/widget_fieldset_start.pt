<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone">
  <head><title></title></head>
  <body>

    <!-- Fieldset Start Widgets -->

    <metal:view_macro define-macro="view">
          <span tal:replace="structure python:here.fgGetFieldsetStart(field, widget, edit=False)" />
    </metal:view_macro>
 
    <metal:define define-macro="edit">
          <span tal:replace="structure python:here.fgGetFieldsetStart(field, widget, edit=True)" />
          <div class="formHelp"
               tal:define="description python:widget.Description(here)"
               tal:content="structure description"
               tal:attributes="id string:${fieldName}_help"
               i18n:translate="">
            Help
          </div>
    </metal:define>

    <div metal:define-macro="search">
          <div>Fieldset Start</div>
    </div>

  </body>

</html>

